{
   "name": "mikaela",
   "version": "1.0.0",
   "description": "Mikaela is DiscordBot who can help make your server a more vibrant place to be in, she plays music, saves members favorites, those who like to debate can even organize their notes and sources with her help.",
   "main": "out/app.js",
   "license": "MIT",
   "author": {
      "name": "Kira",
      "email": "kira.eva@outlook.com"
   },
   "private": true,
   "keywords": [
      "discord",
      "music",
      "debate",
      "typescript",
      "bot"
   ],
   "homepage": "https://github.com/kira0x1/mikaela#readme",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/kira0x1/mikaela.git"
   },
   "bugs": {
      "url": "https://github.com/kira0x1/mikaela/issues"
   },
   "scripts": {
      "start": "node .",
      "start-prod": "node . --env=production",
      "skipdb": "node . --skipDB",
      "testvc": "node . --testvc",
      "prebuild": "npm run clean",
      "build": "tsc",
      "watch": "tsc -w",
      "watch:node": "nodemon .",
      "clean": "rimraf out",
      "clean:node": "rimraf 'node_modules/!(rimraf|.bin)'",
      "clean:all": "npm run clean && npm run clean:node",
      "prettier": "prettier --write **.ts",
      "lint": "eslint . --ext .ts"
   },
   "dependencies": {
      "@discordjs/opus": "^0.5.0",
      "bufferutil": "^4.0.3",
      "chalk": "^4.1.1",
      "discord-ytdl-core": "^5.0.3",
      "discord.js": "^13.0.0-dev.e980948de55e91e59c9e3293ac76bc645a058a53",
      "minimist": "^1.2.5",
      "mongoose": "^5.12.13",
      "ms": "^2.1.3",
      "sodium": "^3.0.2",
      "spotify-uri": "^2.2.0",
      "spotify-url-info": "^2.2.3",
      "string-progressbar": "^1.0.4",
      "tslib": "^2.2.0",
      "utf-8-validate": "^5.0.5",
      "winston": "^3.3.3",
      "ws": "^7.4.6",
      "youtube-sr": "^4.1.3",
      "ytdl-core": "^4.8.2",
      "ytpl": "^2.2.1"
   },
   "devDependencies": {
      "@types/ms": "^0.7.31",
      "@types/node": "^15.12.2",
      "@typescript-eslint/eslint-plugin": "^4.26.1",
      "@typescript-eslint/parser": "^4.26.1",
      "eslint": "^7.28.0",
      "eslint-config-alloy": "^4.1.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.23.4",
      "eslint-plugin-prettier": "^3.4.0",
      "eslint-plugin-unused-imports": "^1.1.1",
      "prettier": "^2.3.1",
      "rimraf": "^3.0.2",
      "typescript": "^4.3.2"
   },
   "nodemonConfig": {
      "events": {
         "start": "cls || clear"
      },
      "delay": 1000
   }
}
